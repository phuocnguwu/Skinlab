        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Huyện/Xã"
        android:textSize="14sp"
        android:layout_marginStart="207dp"
        android:layout_marginTop="360dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />
    <Spinner
        android:id="@+id/huyenxa"
        android:layout_width="146dp"
        android:layout_height="39dp"
        android:layout_marginStart="207dp"
        android:layout_marginTop="386dp"
        android:background="@drawable/roundcorner_edittext"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Địa chỉ cụ thể"
        android:textSize="14sp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="439dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        />

    <EditText
        android:id="@+id/txtDiachicuthe"
        android:layout_width="315dp"
        android:layout_height="63dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="463dp"
        android:background="@drawable/roundcorner_edittext"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />
    <CheckBox
        android:id="@+id/cbdiachimacdinh"
        android:layout_width="315dp"
        android:layout_height="63dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="547dp"
        android:text="Sử dụng làm địa chỉ mặc định"
        android:textSize="14sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="SpeakableTextPresentCheck,TouchTargetSizeCheck" />

    <Button
        android:id="@+id/btnsavediachi"
        android:layout_width="103dp"
        android:layout_height="45.36dp"
        android:layout_marginStart="143dp"
        android:layout_marginTop="616dp"
        android:backgroundTint="#A49CFF"
        android:text="Lưu"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="TouchTargetSizeCheck" />



</androidx.constraintlayout.widget.ConstraintLayout>